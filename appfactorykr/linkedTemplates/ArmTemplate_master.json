{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "appfactorykr"
		},
		"ls_AzureSqlDatabase_apdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase_apdb'"
		},
		"ls_AzureSynapse_kr_sysnpase_ws_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSynapse_kr_sysnpase_ws'"
		},
		"ls_storagegen2kr_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_storagegen2kr'"
		},
		"ls_storagegen2kr_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagegen2kr.dfs.core.windows.net/"
		},
		"ls_databricksws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1029-144442-o2jsgtpe"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "appfactorykr_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureSqlDatabase_apdb_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase_apdb_connectionString')]"
					},
					"ls_AzureSynapse_kr_sysnpase_ws_connectionString": {
						"value": "[parameters('ls_AzureSynapse_kr_sysnpase_ws_connectionString')]"
					},
					"ls_storagegen2kr_accountKey": {
						"value": "[parameters('ls_storagegen2kr_accountKey')]"
					},
					"ls_storagegen2kr_properties_typeProperties_url": {
						"value": "[parameters('ls_storagegen2kr_properties_typeProperties_url')]"
					},
					"ls_databricksws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricksws_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "appfactorykr_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactorykr_ArmTemplate_0')]"
			]
		},
		{
			"name": "appfactorykr_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactorykr_ArmTemplate_1')]"
			]
		},
		{
			"name": "appfactorykr_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactorykr_ArmTemplate_2')]"
			]
		}
	]
}